<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="dmInvitation">

	<insert id="createInvitation" parameterType="DmInvitationDto">
        INSERT INTO dm_invitation (inviter_no, invitee_no, room_no) 
        VALUES (#{inviterNo}, #{inviteeNo}, #{roomNo})
    </insert>
    
</mapper>